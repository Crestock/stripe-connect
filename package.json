{
    "name": "@raceloop/stripe-connect",
    "version": "1.0.0",
    "description": "A Typescript library for interacting with standalone accounts of the Stipe Connect API",
    "private": false,
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/raceloop/stripe.raceloop.git"
    },
    "author": "Jordan Thomas - Raceloop",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/raceloop/stripe.raceloop/issues"
    },
    "homepage": "https://github.com/raceloop/stripe.raceloop#readme",
    "nyc": {
        "check-coverage": true,
        "lines": 80,
        "statements": 80,
        "functions": 80,
        "branches": 80,
        "include": [
            "src/**/*.js"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "all": true
    },
    "dependencies": {
        "request": "2.81.0",
        "request-promise-native": "1.0.4",
        "stripe": "4.24.1",
        "uuid": "3.1.0"
    },
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/chai-as-promised": "0.0.31",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.0.26",
        "@types/request-promise-native": "^1.0.6",
        "@types/sinon": "^2.3.3",
        "@types/stripe": "^3.0.0",
        "@types/uuid": "^3.4.1",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-istanbul": "^1.1.2",
        "gulp-mocha": "^4.3.1",
        "gulp-replace": "^0.6.1",
        "gulp-shell": "^0.6.3",
        "gulp-sourcemaps": "^2.6.1",
        "gulp-tslint": "^8.1.2",
        "gulp-typescript": "^3.2.2",
        "gulpclass": "^0.1.2",
        "mocha": "^3.5.0",
        "mocha-typescript": "^1.1.7",
        "nyc": "^11.2.0",
        "remap-istanbul": "^0.9.5",
        "save-dev": "^2.0.0",
        "sinon": "^3.2.1",
        "sinon-chai": "^2.13.0",
        "source-map-support": "^0.4.17",
        "ts-node": "^3.3.0",
        "tslint": "^5.7.0",
        "tslint-stylish": "^2.1.0",
        "typescript": "^2.5.2"
    },
    "scripts": {
        "test": "gulp tests",
        "compile": "tsc",
        "package": "gulp package",
        "publish": "npm run test && gulp publish",
        "publish-next": "npm run test && gulp publishNext"
    }
}
